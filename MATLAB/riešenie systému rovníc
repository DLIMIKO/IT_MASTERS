% Práca s maticami: 

% Definícia matice:
A = [ 
9 1 3 2 2 1
1 0 -1 9 3 0
-3 -2 1 5 0 2
-2 1 2 0 5 1
3 3 1 -4 0 -1
0 2 3 3 2 0
9 8 7 6 5 4 
]; 

% Definícia vektora pravých strán:
v = [1;0;3;4;0;6;0];

%Riešenie systému rovníc:

risenie = linsolve(A,v);

% Vyjadrenie presnej hodnoty:

riesenie_presne = sym(risenie);

% výpis na obrazovku:

disp(riesenie_presne);



